#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules/ts-node-dev/lib/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules/ts-node-dev/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules/ts-node-dev/lib/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules/ts-node-dev/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.4.2_typescript@5.1.6/node_modules:/Users/rateria/Code/upsc-web-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node-dev/lib/bin.js" "$@"
else
  exec node  "$basedir/../ts-node-dev/lib/bin.js" "$@"
fi
